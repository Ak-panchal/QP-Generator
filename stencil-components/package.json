{
  "name": "stencil-components",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-components/stencil-components.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "start:stencil": "stencil build --watch",
    "build:stencil": "stencil build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "npm run build && build-storybook",
    "story": "npm run build:stencil && npm-run-all --parallel start:stencil storybook"
  },
  "dependencies": {
    "@stencil/core": "^2.0.1",
    "@stencil/react-output-target": "0.0.9"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@storybook/addon-actions": "^6.1.10",
    "@storybook/addon-essentials": "^6.1.10",
    "@storybook/addon-links": "^6.1.10",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addon-viewport": "^6.1.10",
    "@storybook/web-components": "^6.1.10",
    "babel-loader": "^8.2.2",
    "case": "^1.6.3",
    "lit-html": "^1.3.0",
    "npm-run-all": "^4.1.5"
  },
  "keywords": [],
  "author": ""
}
